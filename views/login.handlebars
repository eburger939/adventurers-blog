 <div class="container card-container">
        <div class="row justify-content-center">
            <div class="col-6">
                <div class="all-card">
                    <h5 class="cust-card-header text-center">Already have an account? Sign In!</h5>
                    <div class="card-body custom-body">
                        <form class="login-form">

                            <div class="mb-3">
                                <label for="exampleInputEmail1" class="form-label custom-form-email"><strong>Email
                                        address</strong></label>
                                <input type="email" class="form-control custom-control" id="exampleInputEmail1"
                                    aria-describedby="emailHelp">
                                <div id="emailHelp" class="form-text">We'll never share your email with anyone else.
                                </div>
                            </div>
                            <div class="mb-3">
                                <label for="exampleInputPassword1" class="form-label custom-pw"><strong>Password</strong></label>
                                <input type="password" class="form-control custom-form" id="exampleInputPassword1">
                            </div>

                            <button type="submit" class="btn custom-btn">Log In</button>
                
                                <a href='/register'><span class="register">New to the site?  Click here to register</span></a>
            

                        </form>

                    </div>
                </div>
            </div>

            <script src="./js/login.js"></script>